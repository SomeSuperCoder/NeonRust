_start:
    iter ai, accounts
    next s, ai?
    next r, ai?

    iter ii, ins
    next i, ii?

    cmp i, 1
    je send

    exit -1

send:
    next am, ii?

    cmp s.data.bal, am
    jg send2

    exit -2

send2:
    mov sb, s.data.bal
    mov rb, r.data.bal

    sub sb, am
    add rb, am

    set s.data.bal, sb?
    set r.data.bal, rb?

    exit 0
